{
  "name": "@ba-hub/frontend",
  "version": "0.1.0",
  "type": "module",
  "description": "BA Hub Qwik Frontend",
  "scripts": {
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "build": "qwik build",
    "preview": "qwik build preview && vite preview --open",
    "serve": "tsx server/index.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.5.0",
    "@builder.io/qwik-city": "^1.5.0",
    "fastify": "^4.26.0",
    "@fastify/static": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "tsx": "^4.7.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  }
}
